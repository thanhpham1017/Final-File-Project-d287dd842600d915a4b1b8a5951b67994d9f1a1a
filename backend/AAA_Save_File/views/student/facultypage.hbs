<h1>Faculty page for Student</h1>
<h1>{{facultyData.name}}</h1>
<h1>{{facultyData.description}}</h1>
<h1>----------------------------------------------</h1>

<h1>Event information</h1>
<table border = 1>
{{#each eventData}}
        <tr>
            <td>{{ requirement }}</td>
            <td>{{ formatDate deadline1}}</td>
            <td>{{ formatDate deadline2}}</td>
            <td>
                <a href="/student/eventDetail/{{_id}}">Details</a>
                <a href="/student/submitContribution/{{_id}}">Submit new Contribution</a>
            </td>
        </tr>
    {{/each}}
</table>


