<h1>Faculty</h1>
<table border = 1>
    <a href="/faculty/add">Add</a>
    {{#each facultyList}}
        <tr>
            <td>{{ name }}</td>
            <td>{{description}}</td>
            <td>
                <a href="/faculty/edit/{{_id}}">Edit</a>
                <a href="/faculty/delete/{{_id}}"
                onclick="return confirm('Do you want to delete this faculty: {{faculty}}')" ;>Delete</a>
            </td>
        </tr>
    {{/each}}
</table>