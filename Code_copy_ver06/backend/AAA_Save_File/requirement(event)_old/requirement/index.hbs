<h1>Requirement</h1>
<table border = 1>
    <a href="/requirement/add">Add</a>
    {{#each requirementList}}
        <tr>
            <td>{{ requirement }}</td>
            <td>{{deadline1}}</td>
            <td>{{deadline2}}</td>
            <td>
                <a href="/requirement/edit/{{_id}}">Edit</a>
                <a href="/requirement/delete/{{_id}}"
                onclick="return confirm('Do you want to delete this requirement: {{requirement}}')" ;>Delete</a>
            </td>
        </tr>
    {{/each}}
</table>