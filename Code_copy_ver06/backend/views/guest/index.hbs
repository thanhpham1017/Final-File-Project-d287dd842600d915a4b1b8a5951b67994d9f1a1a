<h1>Guest</h1>
<table border = 1>
    <a href="/guest/add">Add</a>
    {{#each guestList}}
    <tr>
        <td>{{ name }}</td>
        <td>{{ dob }}</td>
        <td>{{ gender }}</td>
        <td>{{ address}}</td>
        <td><img src="data:image/jpeg;base64,{{image}}" alt="Address Image" style="max-width: 100px;"></td>
        <td>{{ user.email }}</td>
        <td>{{ user.password }}</td>
        <td>
            <a href="/guest/edit/{{_id}}">Edit</a>
            <a href="/guest/delete/{{_id}}"
            onclick="return confirm('Do you want to delete ? {{name}}')" ;>Delete</a>
        </td>
    </tr>
    {{/each}}
</table>